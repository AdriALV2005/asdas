generator client {
  provider = "prisma-client-js"
}
 
datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
 
model Employee {
id String @id @default(cuid())
name String
email String
phone String
createdAt DateTime @default(now())
updatedAt DateTime @updatedAt
}


model Contratos {
  codigo_id Int @id @default(autoincrement())
  estado String
  fechaInicio DateTime
  fechaFin DateTime
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}